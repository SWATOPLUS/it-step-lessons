@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>


@if(true)
{
    <p>true @(40 + 2)</p>   
}

@if (false)
{
    <p>false</p>
}

@{ Template("Hello", 7); }

@{
    void Template(string text, int count)
    {
        foreach (var i in Enumerable.Range(0, count))
        {
            <p>@text @i</p>
        }
    }
}